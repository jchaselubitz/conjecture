{
  "name": "drill",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "supabase start",
    "stop": "supabase stop",
    "generate": "kysely-codegen --env-file .env.local",
    "generate-deploy": "kysely-codegen",
    "new": "rm -rf node_modules && rm -rf .next && rm -rf .netlify && yarn cache clean && yarn install && yarn generate",
    "nuke": "yarn install && supabase db reset && supabase start && supabase status && yarn generate",
    "create-migration": "supabase db diff --use-migra -f local_schema",
    "ngrok": "ngrok http --domain=romantic-pig-living.ngrok-free.app 3000",
    "ngrok:server": "ngrok http --domain=romantic-pig-living.ngrok-free.app 54321",
    "dev": "next dev",
    "build-dev": "netlify build --context=development",
    "build-branch": "netlify build --context=branch-deploy",
    "build-staging": "netlify build --context=staging",
    "build-prod": "netlify build --context=production",
    "lint": "yarn run js-lint && yarn run style-lint && yarn run type-check",
    "fix": "yarn run fix-js-lint && yarn run fix-style-lint",
    "fix-js-lint": "eslint -c eslint.config.mjs --fix './app/**/*.{ts,tsx}' './components/**/*.{ts,tsx}' './lib/**/*.{ts,tsx}' './containers/**/*.{ts,tsx}' './contexts/**/*.{ts,tsx}' './*.{ts,tsx}' -f table",
    "js-lint-src": "eslint -c eslint.config.mjs './app/**/*.{ts,tsx}' './components/**/*.{ts,tsx}' './lib/**/*.{ts,tsx}' './containers/**/*.{ts,tsx}' './contexts/**/*.{ts,tsx}' './*.{ts,tsx}' -f table",
    "js-lint-tests": "eslint -c eslint.config.mjs './tests/**/*.{ts,tsx}' -f table",
    "fix-js-lint-tests": "eslint -c eslint.config.mjs --fix './tests/**/*.{ts,tsx}' -f table",
    "fix-style-lint": "stylelint '**/*.css' --fix",
    "js-lint": "yarn run js-lint-src && yarn run js-lint-tests",
    "type-check": "tsc --noEmit",
    "type-coverage": "type-coverage",
    "test": "jest",
    "test:watch": "jest --watch",
    "seed": "npx tsx seed.ts > supabase/seed.sql",
    "seedsync": "npx @snaplet/seed sync"
  },
  "dependencies": {
    "@hookform/resolvers": "^4.1.3",
    "@radix-ui/react-accordion": "^1.2.3",
    "@radix-ui/react-aspect-ratio": "^1.1.2",
    "@radix-ui/react-avatar": "^1.1.3",
    "@radix-ui/react-dialog": "^1.1.6",
    "@radix-ui/react-dropdown-menu": "^2.1.6",
    "@radix-ui/react-hover-card": "^1.1.11",
    "@radix-ui/react-label": "^2.1.2",
    "@radix-ui/react-popover": "^1.1.6",
    "@radix-ui/react-select": "^2.1.6",
    "@radix-ui/react-separator": "^1.1.2",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-switch": "^1.1.3",
    "@radix-ui/react-tabs": "^1.1.9",
    "@radix-ui/react-tooltip": "^1.1.8",
    "@sentry/nextjs": "^9",
    "@supabase/ssr": "^0.5.2",
    "@supabase/supabase-js": "^2.49.1",
    "@tailwindcss/postcss": "^4.0.12",
    "@tailwindcss/typography": "^0.5.16",
    "@tiptap/extension-code-block": "^2.11.5",
    "@tiptap/extension-focus": "^2.11.5",
    "@tiptap/extension-highlight": "^2.11.5",
    "@tiptap/extension-link": "^2.11.5",
    "@tiptap/extension-placeholder": "^2.11.5",
    "@tiptap/extension-typography": "^2.11.5",
    "@tiptap/extension-youtube": "^2.14.0",
    "@tiptap/pm": "^2.11.5",
    "@tiptap/react": "^2.11.5",
    "@tiptap/starter-kit": "^2.11.5",
    "@types/turndown": "^5.0.5",
    "bcryptjs": "^3.0.2",
    "browser-image-compression": "^2.0.2",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "embla-carousel-react": "^8.6.0",
    "katex": "^0.16.21",
    "kysely": "^0.27.6",
    "lucide-react": "^0.479.0",
    "next": "^15.3.3",
    "next-pwa": "^5.6.0",
    "next-themes": "^0.4.6",
    "pg": "^8.13.3",
    "react": "^19.0.0",
    "react-day-picker": "^9.7.0",
    "react-dnd": "^16.0.1",
    "react-dnd-html5-backend": "^16.0.1",
    "react-dom": "^19.0.0",
    "react-hook-form": "^7.54.2",
    "react-ios-keyboard-viewport": "^0.0.1",
    "react-markdown": "^10.1.0",
    "react-resizable-panels": "^2.1.7",
    "react-textarea-autosize": "^8.5.8",
    "react-use": "^17.6.0",
    "sonner": "^2.0.1",
    "tailwind-merge": "^3.0.2",
    "tailwindcss-animate": "^1.0.7",
    "turndown": "^7.2.0",
    "use-debounce": "^10.0.4",
    "vaul": "^1.1.2",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@snaplet/copycat": "^6.0.0",
    "@snaplet/seed": "^0.98.0",
    "@types/katex": "^0.16.7",
    "@types/next-pwa": "^5.6.9",
    "@types/node": "^20",
    "@types/pg": "^8",
    "@types/react": "^19.0.10",
    "@types/react-dom": "^19.0.4",
    "eslint": "^9.22.0",
    "eslint-config-next": "^15.2.1",
    "eslint-formatter-table": "^7.32.1",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-react": "^7.37.2",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "eslint-plugin-sort-imports-es6-autofix": "^0.6.0",
    "kysely-codegen": "^0.18.3",
    "netlify-cli": "^19.1.5",
    "postcss": "^8",
    "prettier": "^3.5.3",
    "tailwindcss": "^4.0.12",
    "typescript": "^5.8.2"
  },
  "packageManager": "yarn@4.9.2"
}
